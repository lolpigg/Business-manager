<Window x:Class="Money.Type"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Money"
        mc:Ignorable="d"
        Title="Type" Height="200" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>

        </Grid.ColumnDefinitions>
        <Button x:Name="Button1" Content="Добавить тип" Grid.Row="2" FontSize="30" Command="{Binding Add_Command}"/>
        <TextBox x:Name="Type1" Grid.Row="1" Text="{Binding TextB}"></TextBox>
        <TextBlock Text="Введите тип" TextAlignment="Center" FontSize="40"></TextBlock>
    </Grid>
</Window>
